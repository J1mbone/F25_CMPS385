#include <iostream>
#include <cstdlib>
#include <ctime>
using namespace std;

int main() {
    int a[5], sum = 0, max;

    srand(time(0));

    for (int i = 0; i < 5; i++) {
        a[i] = rand() % 21;
        sum += a[i];
    }

    max = a[0];
    for (int i = 1; i < 5; i++) {
        if (a[i] > max)
            max = a[i];
    }

    cout << "Array values: ";
    for (int i = 0; i < 5; i++)
        cout << a[i] << " ";

    cout << "\nTotal = " << sum << endl;
    cout << "Maximum = " << max << endl;

    return 0;
}
